{
    "name": "Remote-dev Project",
    "dockerComposeFile": [
      "../docker-compose.yml",
      "docker-compose.extend.yml"
    ],
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash",
      "go.gopath": "/go",
      "go.inferGopath": true,
      "go.useLanguageServer": true
    },
    "extensions": [
      "ms-vscode.go"
    ],
    "service": "app",
    "workspaceFolder": "/work",
    "shutdownAction": "stopCompose"
  }
